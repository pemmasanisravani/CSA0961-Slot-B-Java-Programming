import java.util.*;
class Prime
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n,i,flag=0;
System.out.println("enter the number");
n=sc.nextInt();
for(i=2;i<n;i++)
{
if(n%i==0)
{
flag=flag+1;
}
if(flag>=1)
{
System.out.println("it is composite number"+n);
}
else
{
System.out.println("it is prime number"+n);
break;
}
}
}
}
