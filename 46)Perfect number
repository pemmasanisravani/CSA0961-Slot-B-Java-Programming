class Perfect
{
  public static void main(String arg[])
  {
     int n=6,i,sum=0;
     for(i=1;i<n;i++)
     {
        if(n%i==0)
        { 
          sum=sum+i;
        }
     }
     if(n==sum)
     {
       System.out.println("it is perfect number");
     }
     else
     {
       System.out.println("it is not perfect number");
     }
  }
}
