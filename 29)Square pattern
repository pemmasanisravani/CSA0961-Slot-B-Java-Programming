import java.util.*;
class Squarepattern
{
  public static void main(String arg[])
  {
    Scanner sc=new Scanner(System.in);
    int i,j,k=1,rows;
    System.out.println("enter no of rows:");
    rows=sc.nextInt();
    for(i=1;i<=rows;i++)
    {
      for(j=1;j<=i;j++)
      {
        System.out.print(k*k+" ");
        k++;
      }
      System.out.println();
    }
  }
}
