 import java.util.*;
class Arraysum
{
  public static void main(String arg[])
  {
    Scanner sc=new Scanner(System.in);
    int a[]=new int[5];
    int i,sum=0;
    System.out.println("enter numbers");
    for(i=0;i<5;i++)
    {
      a[i]=sc.nextInt();
      sum=sum+a[i];
    }
    System.out.println("Sum of numbers:"+sum);
  }
}
